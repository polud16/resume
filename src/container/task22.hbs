<main>
    <div>
        {{#with page_header}}
        <div>
            <h1 class="text-center">{{title}}</h1>
        </div>
        <div>
            <p class="text-uppercase text-center">{{description}}</p>
        </div>
        {{/with}}
    </div>
    <br>
    {{#with section}}
    <div class="card">
        <div class="card-body">
            <section>
                {{#with about_us}}
                <div>
                    {{#with title_list}}
                    <div class="display-6 text-end">{{main}}</div>
                    <div>
                        <h2 class="text-end">{{sub}}</h2>
                    </div>
                    {{/with}}
                </div>
                <br>
                <div>
                    {{#with button}}
                    <div>
                        <button class="btn btn-outline-info">{{text}}</button>
                    </div>
                    {{/with}}
                </div>
                {{/with}}
            </section>
        </div>
    </div>
    <br>
    {{#with advantage}}
    <div class="card">
        <div class="card-body">
            <div class="display-5">{{title}}</div>
            <br>
            {{#with efficiency}}
            <div class="card">
                <div class="card-body">
                    <div class="display-4">{{title}}</div>
                    <br>
                    <div>
                        <h6>{{sub_title}}</h6>
                    </div>
                    <br>
                    <div>
                        <p>{{description}}</p>
                    </div>
                    <br>
                    <div>
                        <button class="btn btn-outline-warning">{{button}}</button>
                    </div>
                </div>
            </div>
            {{/with}}
            <br>
            <div class="card">
                <div class="card-body">
                    {{#with innovation}}
                    <div class="display-4 text-center">{{title}}</div>
                    <br>
                    <div class="text-center">
                        <h6>{{sub_title}}</h6>
                    </div>
                    <br>
                    <div class="text-center">
                        <p>{{description}}</p>
                    </div>
                    <br>
                    <div>
                        <button class="btn btn-outline-warning">{{button}}</button>
                    </div>
                    {{/with}}
                </div>
            </div>
            <br>
            <div class="card">
                <div class="card-body">
                    {{#with collaboration}}
                    <div class="display-4 text-end">{{title}}</div>
                    <br>
                    <div class="text-end">
                        <h6>{{sub_title}}</h6>
                    </div>
                    <br>
                    <div class="text-end">
                        <p>{{description}}</p>
                    </div>
                    <br>
                    <div>
                        <button class="btn btn-outline-warning">{{button}}</button>
                    </div>
                    {{/with}}
                </div>
            </div>
        </div>
    </div>
    {{/with}}
    <br>
    {{#with agreement}}
    <div class="card">
        <div class="card-body">
            <div class="display-5">{{title}}</div>
            <br>
            {{#with info}}
            <div class="card">
                <div class="card-body">
                    <div>
                        <h5>{{sub_title}}</h5>
                    </div>
                </div>
            </div>
            <br>
            <div class="card">
                <div class="card-body">
                    {{#with detail}}
                    <div>
                        <p>
                            {{text}}
                        </p>
                    </div>
                    <br>
                    <div>
                        <button class="btn btn-success">{{button}}</button>
                    </div>
                    {{/with}}
                </div>
            </div>
            {{/with}}
        </div>
    </div>
    {{/with}}



    {{/with}}
</main>